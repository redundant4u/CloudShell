{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["es6"],
        "module": "commonjs",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
        "strictPropertyInitialization": false
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
